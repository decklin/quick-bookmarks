{
  "name": "Quick Bookmarks",
  "description": "Access a bookmark folder quickly via popup menu",
  "version": "2.0",
  "icons": { "48": "user-bookmarks.png" },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "book-blue.png",
    "default_title": "Bookmarks",
    "name": "Quick Bookmarks",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ]
}
