{
  "name": "Quick Bookmarks",
  "description": "Access bookmarks via popup menu",
  "version": "3.11",
  "icons": {
      "48": "icons/user-bookmarks-48.png",
      "32": "icons/user-bookmarks-32.png"
  },
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icons/book-blue.png",
    "default_title": "Bookmarks",
    "name": "Quick Bookmarks",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "http://*/*",
    "https://*/*"
  ]
}
